//  rules created by @mukeshbuwade1
{
    "rules": {
        "@typescript-eslint/no-unused-vars": [
            "error",
            {
                "varsIgnorePattern": "^_",
                "args": "after-used"
            }
        ],
        "no-restricted-imports": [
            "error",
            {
                "patterns": [
                    "*.js",
                    "*.jsx"
                ]
            }
        ],
        "import/default": "off"
    }
}